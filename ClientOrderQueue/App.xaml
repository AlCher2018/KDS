<Application x:Class="ClientOrderQueue.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ClientOrderQueue"
             xmlns:locLib="clr-namespace:ClientOrderQueue.Lib"
             >
    <Application.Resources>
        <!-- converters -->
        <locLib:MultiplyParamValueConverter x:Key="multiplyParamValueConverter"/>
        <locLib:GetMargin x:Key="getMargin"/>
        <locLib:GetMinValueConverter x:Key="getMinValueConverter"/>
        <locLib:GetCornerRadiusConverter x:Key="getCornerRadiusConverter"/>
        <locLib:GetDarkerColorConverter x:Key="getDarkerColorConverter"/>
        <locLib:CalculatedMarginConverter x:Key="calculatedMarginConverter" />
    </Application.Resources>
</Application>
