<Application x:Class="KDSWPFClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:KDSWPFClient"
             xmlns:locLib="clr-namespace:KDSWPFClient.View"
             >
    
    <Application.Resources>
        <!-- конвертеры -->
        <locLib:MultiplyParamValueConverter x:Key="multParamConv"/>
        <locLib:GetMargin x:Key="getMargin"/>
        <locLib:GetCornerRadius x:Key="getCornerRadius"/>
        <locLib:IsNullConverter x:Key="isNullConverter"/>
        <locLib:ViewDateConverter x:Key="viewDateConverter"/>
        <locLib:IsZeroConverter x:Key="isZeroConverter"/>
        

        <locLib:BrushConverter x:Key="brushConverter" />

    </Application.Resources>
    
</Application>
